#include <stdlib.h>
#include <stdio.h>

int	*ft_range(int min, int max)
{
	int	i;
	int *len;
	int	*tab;

	*len = max - min;
	if (min >= max)
		return 0;
	tab = ((int*)malloc(sizeof(*tab)) * len + 1);
	i = min;
	while (i <= max)
	{
		tab[i] = i;
		i++;
	}
	return (tab);
}

int	main()
{
	printf("%d\n", ft_range(18, 29));
	return 0;
}
